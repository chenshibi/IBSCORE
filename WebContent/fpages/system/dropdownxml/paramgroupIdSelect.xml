<?xml version="1.0" encoding="UTF-8"?>

<!--  用于直接生成SQL语句，支持同步和异步提交 -->
<!--  title:标题
	navigate:导航信息
	type:方式（sql|call)
	interface:是否显示输入条件页面(true|false)
	databusid:数据总线编号
	transdataactionurl:数据整理URL
	txnlogflag:true记录日志
	async:异步提交标志
	pagesize:页面显示记录数(缺省为10)
-->
<CommQry title="SysParamsEntry.title" navigate="系统参数设置" type="call"
	interface="true" pagesize="10" async="true" databusid=""
	transdataactionurl="/trans/TransDataAction.do"
	txnlogflag="true"
	getterclassname="com.huateng.report.system.getter.ParamgroupIdSelectGetter">
	<Include id="BankParam" />
	<ResultViewParam>/fpages/system/ftl/SysParamsEntry.ftl</ResultViewParam>
	<Fields>
		<!-- status :F-基本信息中显示  N-不显示 D-详细信息中显示 A-基本、详细信息中都显示  -->
		<!-- primary : true-作为表单项记录上传　false-不作为表单项上传 -->
		<Field id="paramgroupId" desc="参数段编号" edittype="text" datatype="string" status="F" size="3" xpath="/id/paramgroupId" valueProtected="true" />
    </Fields>
    <Parameters>
		<Parameter id="paramgroupId" value="" desc="选择"/>
		<Parameter id="value" value="" desc=""/> 
	</Parameters>
</CommQry>