        <meta attribute="sync-DAO">false</meta>
        <id name="id" type="string" column="id"  length="32" >
		</id>
		<property name="batchId" column="BATCHID" type="string" not-null="false" length="30" />
		<property name="zhmc" column="账户名称" type="string" not-null="false" length="30" />
        <property name="sxjg" column="授信机构" type="string" not-null="false" length="30" />
