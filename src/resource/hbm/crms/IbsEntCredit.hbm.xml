        <property name="cjzcwjfl" column="曾经最差五级分类" type="string" not-null="false" length="50" />
        <property name="tue" column="TUE" type="string" not-null="false" length="50" />
        <property name="dsr" column="DSR" type="string" not-null="false" length="50" />
        <property name="wc" column="WC" type="string" not-null="false" length="50" />
    </class>
</hibernate-mapping>
