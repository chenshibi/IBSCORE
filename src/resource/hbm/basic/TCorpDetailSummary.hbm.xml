<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		  name="TCorpDetailSummary"
          table="T_Corp_detail_summary"
          lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="rptKey"
			column="rpt_key"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="createTime"
            column="create_time"
			type="timestamp"
			not-null="false"
			length="14"
		/>
		<property
			name="itemName"
            column="item_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="status"
            column="status"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="organ"
            column="organ"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="count"
            column="count"
			type="integer"
			not-null="false"
			length="20"
		/>
		<property
			name="amount"
            column="amount"
			type="float"
			not-null="false"
			length="30"
		/>
		<property
			name="balance"
            column="balance"
			type="float"
			not-null="false"
			length="30"
		/>
		<property
			name="balanceDay30"
            column="balance_day30"
			type="float"
			not-null="false"
			length="30"
		/>
		<property
			name="balanceDay60"
            column="balance_day60"
			type="float"
			not-null="false"
			length="30"
		/>
		<property
			name="balanceDay90"
            column="balance_day90"
			type="float"
			not-null="false"
			length="30"
		/>
		<property
			name="balanceDay90Plus"
            column="balance_day90_plus"
			type="float"
			not-null="false"
			length="30"
		/>
		<property
			name="bizDetail"
            column="biz_detail"
			type="string"
			not-null="false"
			length="20"
		/>
		
	</class>
</hibernate-mapping>