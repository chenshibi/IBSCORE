<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		name="FunctionInfo"
		table="FUNCTION_INFO"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="FUNCID"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="funcname"
			column="FUNCNAME"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="pagepath"
			column="PAGEPATH"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="location"
			column="LOCATION"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="isdirectory"
			column="ISDIRECTORY"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="lastdirectory"
			column="LASTDIRECTORY"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="showseq"
			column="SHOWSEQ"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="funcClass"
			column="FUNC_CLASS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="funcType"
			column="FUNC_TYPE"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="workflowFlag"
			column="WORKFLOW_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="upFuncCode"
			column="UP_FUNC_CODE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="funcDesc"
			column="FUNC_DESC"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="effectDate"
			column="EFFECT_DATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="expireDate"
			column="EXPIRE_DATE"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="timestamps"
			column="TIMESTAMPS"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="miscflgs"
			column="MISCFLGS"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="misc"
			column="MISC"
			type="string"
			not-null="false"
			length="1024"
		/>
		<property
			name="iconCls"
			column="ICON_CLS"
			type="string"
			not-null="false"
			length="256"
		/>

	</class>
</hibernate-mapping>