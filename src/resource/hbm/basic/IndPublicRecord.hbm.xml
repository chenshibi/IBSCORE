<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
    <class
            name="IndPublicRecord"
            table="Ind_public_record"
            lazy="false"
    >
    <meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		<generator class ="increment"/>
		</id>
		<property
			name="rptId"
			column="Rpt_id"
			type="string"
			not-null="false"
			length="22"
		/>

        <property
                name="endDate"
                column="end_date"
                type="string"
                not-null="false"
                length="30"
        />
       
        <property
                name="initDate"
                column="init_date"
                type="string"
                not-null="false"
                length="30"
        />
        <property
                name="no"
                column="no"
                type="integer"
                not-null="false"
                length="20"
        />
        <property
                name="objectAmount"
                column="object_amount"
                type="string"
                not-null="false"
                length="20"
        />
         <property
                name="objectName"
                column="object_name"
                type="string"
                not-null="false"
                length="100"
        />
        <property
                name="organ"
                column="organ"
                type="string"
                not-null="false"
                length="100"
        />
        <property
                name="reason"
                column="reason"
                type="string"
                not-null="false"
                length="300"
        />
        <property
                name="recordType"
                column="record_type"
                type="string"
                not-null="false"
                length="20"
        />
         <property
                name="result"
                column="result"
                type="string"
                not-null="false"
                length="300"
        />
         <property
                name="status"
                column="status"
                type="string"
                not-null="false"
                length="20"
        />
        <property
                name="subjectAmount"
                column="subject_amount"
                type="string"
                not-null="false"
                length="20"
        />
         <property
                name="subjectName"
                column="subject_name"
                type="string"
                not-null="false"
                length="100"
        />
        <property
                name="type"
                column="type"
                type="string"
                not-null="false"
                length="30"
        />
        
    </class>
</hibernate-mapping>