<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="resource.bean.basic">
	<class
		name="PfSysParam"
		table="PF_SYS_PARAM"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="id" class="PfSysParamPK">
			<key-property
				name="magicId"
				column="MAGIC_ID"
				type="string"
			/>
			<key-property
				name="paramId"
				column="PARAM_ID"
				type="string"
			/>
		</composite-id>

		<property
			name="paramStartTm"
			column="PARAM_START_TM"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="paramEndTm"
			column="PARAM_END_TM"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="paramChangFlag"
			column="PARAM_CHANG_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="paramValueTx"
			column="PARAM_VALUE_TX"
			type="string"
			not-null="false"
			length="512"
		/>
		<property
			name="lastUpdTlr"
			column="LAST_UPD_TLR"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="lastUpdFunc"
			column="LAST_UPD_FUNC"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="lastUpdDate"
			column="LAST_UPD_DATE"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="desc0"
			column="DESC0"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="st"
			column="ST"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="lock"
			column="IS_LOCK"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="del"
			column="IS_DEL"
			type="string"
			not-null="false"
			length="1"
		/>

	</class>
</hibernate-mapping>