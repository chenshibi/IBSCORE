        <property
                name="rptKey"
                column="rpt_key"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="batchId"
                column="batch_id"
                type="integer"
                not-null="false"
                length="14"
        />
        
        <property
                name="name"
                column="name"
                type="string"
                not-null="false"
                length="14"
        />
        
         <property
                name="loancard"
                column="loancard"
                type="string"
                not-null="false"
                length="14"
        />
        
        
        <property
                name="password"
                column="password"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="orgcode"
                column="orgcode"
                type="string"
                not-null="false"
                length="14"
        />
        <property
                name="loanNo"
                column="loan_no"
                type="string"
                not-null="false"
                length="14"
        />
        
        
         <property
                name="status"
                column="status"
                type="string"
                not-null="false"
                length="14"
        />
        
         <property
                name="queryReason"
                column="query_reason"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="createUser"
                column="create_user"
                type="string"
                not-null="false"
                length="14"
        />
        
        <property
                name="inputTime"
                column="input_time"
                type="timestamp"
                not-null="false"
                length="14"
        />
        <property
                name="returnTime"
                column="return_time"
                type="timestamp"
                not-null="false"
                length="14"
        />
        <property
                name="detailAppId"
                column="detail_app_id"
                type="integer"
                not-null="false"
                length="14"
        />
         <property
                name="appId"
                column="app_id"
                type="integer"
                not-null="false"
                length="14"
        />
    </class>
</hibernate-mapping>