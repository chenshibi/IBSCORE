		<generator class ="increment"/>
		</id>
		<property
			name="rptKey"
			column="rpt_key"
			type="string"
			not-null="false"
			length="14"
		/>

        <property
                name="contractType"
                column="contract_type"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="no"
                column="no"
                type="string"
                not-null="false"
                length="14"
        />
       
        <property
                name="contractNo"
                column="contract_no"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="pledgeNo"
                column="pledge_no"
                type="string"
                not-null="false"
                length="14"
        />
       <property
                name="pledgeAmount"
                column="pledge_amount"
                type="float"
                not-null="false"
                length="14"
        />
         <property
                name="pledgeCurrency"
                column="pledge_currency"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="pledgeEstAmount"
                column="pledge_est_amount"
                type="float"
                not-null="false"
                length="14"
        />
         <property
                name="pledgeEstCurrency"
                column="pledge_est_currency"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="estDate"
                column="est_date"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="estOrgName"
                column="est_org_name"
                type="string"
                not-null="false"
                length="14"
        />
        <property
                name="estOrgCode"
                column="est_org_code"
                type="string"
                not-null="false"
                length="14"
        />
        <property
                name="pledgeType"
                column="pledge_type"
                type="string"
                not-null="false"
                length="14"
        />
        <property
                name="registerOrg"
                column="register_org"
                type="string"
                not-null="false"
                length="14"
        />
        <property
                name="registerDate"
                column="register_date"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="pledgeDesc"
                column="pledge_desc"
                type="string"
                not-null="false"
                length="14"
        />
        
    </class>
</hibernate-mapping>