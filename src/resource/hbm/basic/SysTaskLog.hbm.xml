<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		name="SysTaskLog"
		table="SYS_TASK_LOG"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="TSK_ID"
		>
		</id>

		<property
			name="intOperId"
			column="INT_OPER_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="insCd"
			column="INS_CD"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="intInsId"
			column="INT_INS_ID"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="adtRcdPk"
			column="ADT_RCD_PK"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="updTransCd"
			column="UPD_TRANS_CD"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="crtDt"
			column="CRT_DT"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="lastUpdOper"
			column="LAST_UPD_OPER"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="lastUpdTms"
			column="LAST_UPD_TMS"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="newVal1"
			column="NEW_VAL1"
			type="string"
			not-null="false"
		/>
		<property
			name="newVal2"
			column="NEW_VAL2"
			type="string"
			not-null="false"
		/>
		<property
			name="approveResult"
			column="APPROVE_RESULT"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="approveOperId"
			column="APPROVE_OPER_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="approveInsCd"
			column="APPROVE_INS_CD"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="approveTm"
			column="APPROVE_TM"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="approveRemark"
			column="APPROVE_REMARK"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="oldVal1"
			column="OLD_VAL1"
			type="string"
			not-null="false"
		/>
		<property
			name="oldVal2"
			column="OLD_VAL2"
			type="string"
			not-null="false"
		/>


	</class>	
</hibernate-mapping>