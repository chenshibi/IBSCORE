<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping  package="resource.bean.basic">
    <class name="PasswordHis" table="PASSWORD_HIS" >
        <id name="id" type="java.lang.String">
            <column name="ID" />
            <generator class="uuid" />
        </id>
        <property name="userid" type="string">
            <column name="USERID" length="32" not-null="true" />
        </property>
        <property name="password" type="java.lang.String">
            <column name="PASSWORD" length="100" not-null="true" />
        </property>
        <property name="encMode" type="java.lang.String">
            <column name="ENC_MODE" length="10" not-null="true" />
        </property>
        <property name="modifiedTime" type="string">
            <column name="MODIFIED_TIME" length="14" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
