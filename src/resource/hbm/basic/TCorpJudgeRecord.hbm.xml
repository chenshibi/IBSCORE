<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		  name="TCorpJudgeRecord"
          table="T_Corp_judge_record"
          lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="rptKey"
			column="rpt_key"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="createTime"
            column="create_time"
			type="timestamp"
			not-null="false"
			length="14"
		/>
		<property
			name="itemName"
            column="item_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="court"
            column="court"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="judgeDate"
            column="judge_date"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="judgeReason"
            column="judge_reason"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="judgeParty"
            column="judge_party"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="caseNo"
            column="case_no"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="judgeProcess"
            column="judge_process"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="judgeObject"
            column="judge_object"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="objectValue"
            column="object_value_"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="caseCloseMode"
            column="case_close_mode"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="judgeEffDate"
            column="judge_eff_date"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="judgeResult"
            column="judge_result"
			type="string"
			not-null="false"
			length="500"
		/>
		<property
			name="caseStatus"
            column="case_status"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="executedObject"
            column="executed_object"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="executedObjectValue"
            column="executed_object_value"
			type="string"
			not-null="false"
			length="100"
		/>
		
	</class>
</hibernate-mapping>