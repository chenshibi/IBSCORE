<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		  name="TCorpGuaranteedDetail"
          table="T_Corp_guaranteed_detail"
          lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="rptKey"
			column="rpt_key"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="createTime"
            column="create_time"
			type="timestamp"
			not-null="false"
			length="14"
		/>
		<property
			name="itemName"
            column="item_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="type"
            column="type"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="assurePerson"
            column="assure_person"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="assureKind"
            column="assure_kind"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="assureDate"
            column="assure_date"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="currency"
            column="currency"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="valuation"
            column="valuation"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="assureAmt"
            column="assure_amt"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="assureType"
            column="assure_type"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="fiveLevel"
            column="five_level"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="assuredName"
            column="assured_name"
			type="string"
			not-null="false"
			length="20"
		/>
	</class>
</hibernate-mapping>