<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		name="BiTlrFavt"
		table="BI_TLR_FAVT"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
		>
		</id>

		<property
			name="tlrNo"
			column="TLR_NO"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="funcId"
			column="FUNC_ID"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="showSeq"
			column="SHOW_SEQ"
			type="integer"
			not-null="false"
			length="22"
		/>
		<property
			name="funcType"
			column="FUNC_TYPE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="brNo"
			column="BR_NO"
			type="string"
			not-null="false"
			length="20"
		/>

	</class>
</hibernate-mapping>