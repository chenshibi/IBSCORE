<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
    <class
            name="IndEnquiry"
            table="Ind_enquiry"
            lazy="false"
    >
    <meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		<generator class ="increment"/>
		</id>
		<property
			name="rptId"
			column="Rpt_id"
			type="string"
			not-null="false"
			length="22"
		/>

        <property
                name="enqDate"
                column="enq_date"
                type="timestamp"
                not-null="false"
                length="18"
        />
         <property
                name="enquirer"
                column="enquirer"
                type="string"
                not-null="false"
                length="200"
        />
       
        <property
                name="reason"
                column="reason"
                type="string"
                not-null="false"
                length="100"
        />
        <property
                name="no"
                column="no"
                type="integer"
                not-null="false"
                length="20"
        />
        
    </class>
</hibernate-mapping>