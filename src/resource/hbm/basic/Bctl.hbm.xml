			name="otherAreaFlag"
			column="OTHER_AREA_FLAG"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="regionalism"
			column="REGIONALISM"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="financeCode"
			column="FINANCE_CODE"
			type="string"
			not-null="false"
			length="14"
		/>
		<property
			name="status"
			column="STATUS"
			type="string"
			not-null="false"
