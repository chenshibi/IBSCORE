<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		name="Bctl"
		table="BCTL"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="brcode"
			type="string"
			column="BRCODE"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="brno"
			column="BRNO"
			type="string"
			not-null="true"
			length="20"
		/>
		<property
			name="brname"
			column="BRNAME"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="brclass"
			column="BRCLASS"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="brattr"
			column="BRATTR"
			type="string"
			not-null="false"
