<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		  name="TCorpSocialPayRecord"
          table="T_Corp_social_pay_record"
          lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="rptKey"
			column="rpt_key"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="createTime"
            column="create_time"
			type="timestamp"
			not-null="false"
			length="14"
		/>
		<property
			name="itemName"
            column="item_name"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="sumYm"
            column="sum_ym"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="payAmt"
            column="pay_amt"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="payStatus"
            column="pay_status"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="sumOwnAmt"
            column="sum_own_amt"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="employeeNumber"
            column="employee_number"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="firstPayYm"
            column="first_pay_ym"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="lastPayDate"
            column="last_pay_date"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="lastPayYm"
            column="last_pay_ym"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="insuranceType"
            column="insurance_type"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="insuredDate"
            column="insured_date"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="organ"
            column="organ"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="infotype"
            column="infotype"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="highestOwnDate"
            column="highest_own_date"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="highestOwnAmt"
            column="highest_own_amt"
			type="string"
			not-null="false"
			length="100"
		/>
		
	</class>
</hibernate-mapping>