			not-null="false"
			length="30"
		/>
		<property
			name="divestAmount"
            column="divest_amount"
			type="float"
			not-null="false"
			length="30"
		/>
		<property
			name="divestDate"
            column="divest_date"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="advancedCount"
            column="advanced_count"
			type="integer"
			not-null="false"
			length="20"
