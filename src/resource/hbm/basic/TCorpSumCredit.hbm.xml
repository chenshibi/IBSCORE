			name="assureCreditCount"
            column="assure_credit_count"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="assureCreditSum"
            column="assure_credit_sum"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="debitInterestClearDate"
            column="debit_interest_clear_date"
			type="string"
			not-null="false"
			length="30"
		/>
		
	</class>
</hibernate-mapping>