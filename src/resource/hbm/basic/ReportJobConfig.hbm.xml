<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class name="ReportJobConfig" table="REPORT_JOB_CONFIG">
		<meta attribute="sync-DAO">false</meta>
		<id name="id" type="string" column="JOB_ID"></id>
		
		<property name="jobName" column="JOB_NAME" type="string" not-null="false" length="128"/>
		<property name="jobClassName" column="JOB_CLASS_NAME" type="string" not-null="false" length="256"/>
		<property name="jobTime" column="JOB_TIME" type="string" not-null="false" length="64"/>
		<property name="jobStauts" column="JOB_STAUTS" type="string" not-null="false" length="1"/>
		<property name="justWorkdateRun" column="JUST_WORKDATE_RUN" type="string" not-null="false" length="1"/>
		<property name="jobRemark" column="JOB_REMARK" type="string" not-null="false" length="256"/>
		<property name="jobLstTm" column="JOB_LST_TM" type="string" not-null="false" length="14"/>
		<property name="jobLstTrl" column="JOB_LST_TRL" type="string" not-null="false" length="20"/>
		<property name="sysName" column="SYSNAME" type="string" not-null="false" length="20"/>
	</class>	
</hibernate-mapping>