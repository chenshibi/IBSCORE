<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
    <class
            name="IndOverdue"
            table="Ind_Overdue"
            lazy="false"
    >
    <meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		<generator class ="increment"/>
		</id>
		<property
			name="rptId"
			column="Rpt_id"
			type="string"
			not-null="false"
			length="22"
		/>

        <property
                name="loanCount"
                column="loan_count"
                type="integer"
                not-null="false"
                length="20"
        />
       
        <property
                name="loanMonthCount"
                column="loan_month_count"
                type="integer"
                not-null="false"
                length="20"
        />
        <property
                name="loanMaxAmount"
                column="loan_max_amount"
                type="float"
                not-null="false"
                length="20"
        />
         <property
                name="loanMaxMonth"
                column="loan_max_month"
                type="integer"
                not-null="false"
                length="20"
        />
        <property
                name="ccCount"
                column="cc_count"
                type="integer"
                not-null="false"
                length="20"
        />
        <property
                name="ccMonthCount"
                column="cc_month_count"
                type="integer"
                not-null="false"
                length="20"
        />
         <property
                name="ccMaxAmount"
                column="cc_max_amount"
                type="float"
                not-null="false"
                length="20"
        />
        <property
                name="ccMaxMonth"
                column="cc_max_month"
                type="integer"
                not-null="false"
                length="20"
        />
         <property
                name="pdcCount"
                column="pdc_count"
                type="integer"
                not-null="false"
                length="20"
        />
         <property
                name="pdcMonthCount"
                column="pdc_month_count"
                type="integer"
                not-null="false"
                length="20"
        />
         <property
                name="pcdMaxAmount"
                column="pcd_max_amount"
                type="float"
                not-null="false"
                length="20"
        />
         <property
                name="pdcMaxMonth"
                column="pdc_max_month"
                type="integer"
                not-null="false"
                length="20"
        />
        <property
                name="getDate"
                column="get_date"
                type="timestamp"
                not-null="false"
                length="18"
        />
        
    </class>
</hibernate-mapping>