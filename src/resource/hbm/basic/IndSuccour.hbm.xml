<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
    <class
            name="IndSuccour"
            table="Ind_succour"
            lazy="false"
    >
    <meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		<generator class ="increment"/>
		</id>
		<property
			name="rptId"
			column="Rpt_id"
			type="string"
			not-null="false"
			length="22"
		/>

        <property
                name="applyDate"
                column="apply_date"
                type="string"
                not-null="false"
                length="30"
        />
       
        <property
                name="city"
                column="city"
                type="string"
                not-null="false"
                length="100"
        />
        <property
                name="familyIncome"
                column="family_income"
                type="string"
                not-null="false"
                length="20"
        />
        <property
                name="issueDate"
                column="issue_date"
                type="string"
                not-null="false"
                length="30"
        />
         <property
                name="no"
                column="no"
                type="integer"
                not-null="false"
                length="20"
        />
         <property
                name="organ"
                column="organ"
                type="string"
                not-null="false"
                length="100"
        />
         <property
                name="type"
                column="type"
                type="string"
                not-null="false"
                length="30"
        />
        <property
                name="updateDate"
                column="update_date"
                type="string"
                not-null="false"
                length="30"
        />
        
    </class>
</hibernate-mapping>