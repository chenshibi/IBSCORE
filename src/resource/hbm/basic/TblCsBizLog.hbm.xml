<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		name="TblCsBizLog"
		table="TBL_CS_BIZ_LOG"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="string"
			column="ID"
			length="32"
		>
			<generator class="uuid" />
		</id>

		<property
			name="logId"
			column="LOG_ID"
			type="string"
			not-null="true"
			length="36"
		/>
		<property
			name="txnDate"
			column="TXN_DATE"
			type="string"
			not-null="true"
			length="8"
		/>
		<property
			name="txnStartTime"
			column="TXN_START_TIME"
			type="string"
			not-null="true"
			length="6"
		/>
		<property
			name="txnEndTime"
			column="TXN_END_TIME"
			type="string"
			not-null="true"
			length="6"
		/>
		<property
			name="txnRunTime"
			column="TXN_RUN_TIME"
			type="long"
			not-null="true"
			length="6"
		/>
		<property
			name="oprCode"
			column="OPRCODE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="ipAdr"
			column="IP_ADR"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="funcId"
			column="FUNCID"
			type="string"
			not-null="false"
			length="8"
		/>
		<property
			name="oprTxnCd"
			column="OPRTXNCD"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="txnBizLog1"
			column="TXN_BIZ_LOG1"
			type="string"
			not-null="false"
			length="2048"
		/>
		<property
			name="txnBizLog2"
			column="TXN_BIZ_LOG2"
			type="string"
			not-null="false"
			length="2048"
		/>
		<property
			name="txnStatus"
			column="TXN_STATUS"
			type="string"
			not-null="false"
			length="2"
		/>
		<property
			name="txnFailLog"
			column="TXN_FAIL_LOG"
			type="string"
			not-null="false"
			length="2048"
		/>

	</class>
</hibernate-mapping>