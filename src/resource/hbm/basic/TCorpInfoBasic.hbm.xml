<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
	<class
		  name="TCorpInfoBasic"
          table="T_Corp_Info_Basic"
          lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="rptKey"
			column="rpt_key"
			type="string"
			not-null="true"
			length="50"
		/>
		<property
			name="createTime"
            column="create_time"
			type="timestamp"
			not-null="false"
			length="14"
		/>
		<property
			name="name"
            column="name"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="address"
            column="address"
			type="string"
			not-null="false"
			length="300"
		/>
		<property
			name="regOrganType"
            column="reg_organ_type"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="loancardId"
            column="loancard_id"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="regOrganNo"
            column="reg_organ_no"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="regOrganCode"
            column="reg_organ_code"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="regDate"
            column="reg_date"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="regEndDate"
            column="reg_end_date"
			type="string"
			not-null="false"
			length="30"
		/>
		<property
			name="regStateTaxNo"
            column="reg_state_tax_no"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="regLocalTaxNo"
            column="reg_local_tax_no"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="loanCardStatus"
            column="loan_card_status"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="lastAuditDate"
            column="last_audit_date"
			type="string"
			not-null="false"
			length="30"
		/>
		
	</class>
</hibernate-mapping>