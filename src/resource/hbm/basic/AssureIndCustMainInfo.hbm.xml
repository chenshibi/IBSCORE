<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="resource.bean.basic">
    <class
            name="AssureIndMainInfo"
            table="Assure_Ind_Main_Info"
            lazy="false"
    >
    <meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="id"
		>
		<generator class ="increment"/>
		</id>
		<property
			name="rptKey"
			column="rpt_key"
			type="string"
			not-null="false"
			length="14"
		/>

        <property
                name="assureType"
                column="assure_type"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="no"
                column="no"
                type="string"
                not-null="false"
                length="14"
        />
       
        <property
                name="contractNo"
                column="contract_no"
                type="string"
                not-null="false"
                length="14"
        />
         <property
                name="bizType"
                column="biz_type"
                type="string"
                not-null="false"
                length="14"
        />
       <property
                name="bizAmount"
                column="biz_amount"
                type="float"
                not-null="false"
                length="14"
        />
        <property
                name="bizBalance"
                column="biz_balance"
                type="float"
                not-null="false"
                length="14"
        />
        
        
    </class>
</hibernate-mapping>